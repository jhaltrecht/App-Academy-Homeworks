<h1>New Cat Rental Request</h1>
<form action= "<%=cat_rental_requests_url %>" method="post">
<label for="cat_rented">Rented Cat</label>
<select name="cat_rental_request[cat_id]" id="cat_rented">
<option disabled selected>Choose a Cat</option>
<% Cat.all.each do |cat| %>
<option value="<%= cat.id %>"><%= cat.name %></option>
<% end %>
</select>
<br>
<label for="start_date">Start Date</label>
<input type="date" name="cat_rental_request[start_date]" id="start_date">
<label for="end_date">End Date</label>
<input type="date" name="cat_rental_request[end_date]" id="end_date">

<br>

<label for="submit_button">Submit Request</label>
<input type="submit" id=submit_button>